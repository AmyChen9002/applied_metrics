\frametitle{Monte Carlo Example}
\begin{itemize}
\item Let's integrate $g(x) = \phi(x)$ (normal pdf) over $f(x) = Unif(0,1)$ from $[0,1]$.
\item We know the answer is $\Phi(1) - \Phi(0)$.
\end{itemize}
\begin{minted}{R}
Integral <- function(n){
    X <- runif(n)
    Y <- exp(-X^2/2)/sqrt(2*pi)
    Int <- sum(Y)/n
    Error <- Int-(pnorm(1)-pnorm(0))
    list(Int,Error)}
\end{minted}

